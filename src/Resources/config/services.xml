<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Sg\ProdConfigurator\DAL\Configuration\ConfigurationDefinition">
            <tag name="shopware.entity.definition" entity="sg_prod_configurator"/>
        </service>

        <service id="Sg\ProdConfigurator\DAL\Flow\FlowDefinition">
            <tag name="shopware.entity.definition" entity="sg_prod_flow"/>
        </service>

        <service id="Sg\ProdConfigurator\DAL\Step\StepDefinition">
            <tag name="shopware.entity.definition" entity="sg_step"/>
        </service>

        <service id="Sg\ProdConfigurator\DAL\Step\Aggregate\StepTypeDefinition">
            <tag name="shopware.entity.definition" entity="sg_step_type"/>
        </service>

        <service id="Sg\ProdConfigurator\DAL\Configuration\Extension\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>

    </services>
</container>
